<% layout("/layouts/boilerplate2") %>
    <%- include('../includes/searchpage-nav.ejs') %>
        <div class="container text-center">
            <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4">
                <% for(let image of images.value) { %>
                    <div class="col">
                        <div class="card imageS" style="width: 15rem;">
                            <a href="<%= image.hostPageUrl %>" class="img-link">
                                <img src="<%= image.contentUrl %>" class="search-img card-img-top" alt="laptop">
                                <p class="img-content card-text">
                                    <%= image.name%>
                                </p>
                            </a>
                        </div>
                    </div>
                    <% } %>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
            crossorigin="anonymous"></script>
        <%- include('../includes/footer.ejs') %>